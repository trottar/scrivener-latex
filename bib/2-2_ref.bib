
@misc{yero_cross_2020,
	title = {Cross {Section} {Measurements} of {Deuteron} {Electro}-{Disintegration} at {Very} {High} {Recoil} {Momenta} and {Large} 4-{Momentum} {Transfers} (\${Q}{\textasciicircum}\{2\}\$)},
	url = {http://arxiv.org/abs/2009.11343},
	abstract = {The \${\textasciicircum}\{2\}\$H\$(e,e'p)n\$ cross sections have been measured at negative 4-momentum transfers of \$Q{\textasciicircum}\{2\} = 4.5{\textbackslash}pm0.5\$ (GeV/c)\${\textasciicircum}\{2\}\$ and \$Q{\textasciicircum}\{2\} = 3.5{\textbackslash}pm0.5\$ (GeV/c)\${\textasciicircum}\{2\}\$ reaching neutron recoil (missing) momenta up to \$p\_\{{\textbackslash}mathrm\{r\}\}{\textbackslash}sim\$1.0 GeV/c. The data have been obtained at fixed neutron recoil angles \$5{\textasciicircum}\{{\textbackslash}circ\}{\textbackslash}leq{\textbackslash}theta\_\{nq\}{\textbackslash}leq95{\textasciicircum}\{{\textbackslash}circ\}\$ with respect to the 3-momentum transfer \${\textbackslash}vec\{q\}\$. The new data agree well with the previous data which reached \$p\_\{{\textbackslash}mathrm\{r\}\}{\textbackslash}sim550\$ MeV/c. At \${\textbackslash}theta\_\{nq\}=35{\textasciicircum}\{{\textbackslash}circ\}\$ and \$45{\textasciicircum}\{{\textbackslash}circ\}\$, final state interactions (FSI), meson exchange currents (MEC) and isobar configurations (IC) are suppressed and the plane wave impulse approximation (PWIA) provides the dominant cross section contribution. The new data are compared to recent theoretical calculations, and a significant disagreement for recoil momenta \$p\_\{{\textbackslash}mathrm\{r\}\}{\textgreater}700\$ MeV/c is observed.},
	language = {en},
	urldate = {2022-09-20},
	publisher = {arXiv},
	author = {Yero, Carlos},
	month = sep,
	year = {2020},
	note = {arXiv:2009.11343 [nucl-ex]},
	keywords = {Nuclear Experiment},
	annote = {Comment: (in v2 of this document I added the following comments): PhD Thesis in Experimental Nuclear Physics (Florida International University); 271 pages (excluding title page/table of contents), 158 figures, 36 tables; For a concise discussion of the experimental results, see arXiv:2008.08058},
	file = {Yero - 2020 - Cross Section Measurements of Deuteron Electro-Dis.pdf:C\:\\Users\\papatrott\\Zotero\\storage\\7IXDMZ65\\Yero - 2020 - Cross Section Measurements of Deuteron Electro-Dis.pdf:application/pdf},
}

