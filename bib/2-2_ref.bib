@article{yan_superharp_1995,
	title = {Superharp — {A} wire scanner with absolute position readout for beam energy measurement at {CEBAF}},
	volume = {365},
	issn = {0168-9002},
	url = {https://www.sciencedirect.com/science/article/pii/0168900295005056},
	doi = {10.1016/0168-9002(95)00505-6},
	abstract = {The CEBAF superharp is an upgraded beam wire scanner which provides absolute beam position readout using a shaft encoder. Superharps allow for high precision measurements of the beam's profile and position (Δx ∼ 10 μm). The Hall C endstation at CEBAF will use three pairs of superharps to perform beam energy measurements with 10−3 accuracy. The three pairs are installed at the beginning, the mid-point and the end of the Hall C arc beamline. Using superharps in conjunction with a dual sensor system: the direct current pick-up and the bremsstrahlung detectors, beam profile measurements can be obtained over a wide beam current range of 1 ∼ 200 μA.},
	language = {en},
	number = {2},
	urldate = {2023-05-21},
	journal = {Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
	author = {Yan, C. and Adderley, P. and Barker, D. and Beaufait, J. and Capek, K. and Carlini, R. and Dahlberg, J. and Feldl, E. and Jordan, K. and Kross, B. and Oren, W. and Wojcik, R. and VanDyke, J.},
	month = nov,
	year = {1995},
	pages = {261--267},
	file = {1-s2.0-0168900295005056-main.pdf:C\:\\Users\\papatrott\\Zotero\\storage\\XWVV49MG\\1-s2.0-0168900295005056-main.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\papatrott\\Zotero\\storage\\D8HZP965\\0168900295005056.html:text/html},
