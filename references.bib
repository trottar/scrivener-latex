	file = {Full Text PDF:C\:\\Users\\papatrott\\Zotero\\storage\\ZFV3M5XH\\Qin et al. - 2018 - Off-shell persistence of composite pions and kaons.pdf:application/pdf;Qin et al. - 2018 - Off-shell persistence of composite pions and kaons.pdf:C\:\\Users\\papatrott\\Zotero\\storage\\XU9ZIAJJ\\Qin et al. - 2018 - Off-shell persistence of composite pions and kaons.pdf:application/pdf},
