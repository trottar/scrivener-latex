	annote = {Comment: 15 pages, 7 figures. Contribution to the Proceedings of the 37th Brazilian Workshop on Nuclear Physics, Maresias, SP, Brazil},
	file = {arXiv Fulltext PDF:C\:\\Users\\papatrott\\Zotero\\storage\\KPQ5AGQS\\Roberts - 2015 - Hadron Physics and QCD Just the Basic Facts.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\papatrott\\Zotero\\storage\\VFFZEW3N\\1501.html:text/html},
