	doi = {10.48550/arXiv.1712.03214},
	abstract = {This work is a pioneering study of backward-angle omega cross sections through the exclusive 1H(e,e'p)omega reaction using the missing mass reconstruction technique. The extracted cross sections are separated into the transverse (T), longitudinal (L), and LT, TT interference terms. The analyzed data were part of experiment E01-004 (Fpi-2), which used 2.6-5.2GeV electron beams and HMS+SOS spectrometers in Jefferson Lab Hall C. The primary objective was to detect coincidence pion in the forward-angle, where the backward-angle omega events were fortuitously detected. The experiment has central Q2 values of 1.60 and 2.45GeV2, at W=2.21GeV. There was significant coverage in phi and epsilon, which allowed separation of sigma\_T,L,LT,TT. The data set has a unique u coverage of -u{\textasciitilde}0, which corresponds to -t{\textgreater}4GeV2. The separated sigma\_T result suggest a flat {\textasciitilde}1/Q{\textasciicircum}(1.33+/-1.21) dependence, whereas sigma\_L seems to hold a stronger 1/Q{\textasciicircum}(9.43+/-6.28) dependence. The sigma\_L/sigma\_T ratio indicate sigma\_T dominance at Q2=2.45 GeV2 at the {\textasciitilde}90\% confidence level. After translating the results into the -t space of the published CLAS data, our data show evidence of a backward-angle omega electroproduction peak at both Q2 settings. Previously, this phenomenon showing both forward and backward-angle peaks was only observed in the meson photoproduction data. Through comparison of our sigma\_T data with the prediction of the Transition Distribution Amplitude (TDA) model, and signs of sigma\_T dominance, promising indications of the applicability of the TDA factorization are demonstrated at a much lower Q2 value than its preferred range of Q2{\textgreater}10GeV2. These studies have opened a new means to study the transition of the nucleon wavefunction through backward-angle experimental observables.},
	urldate = {2024-02-26},
	publisher = {arXiv},
	author = {Li, Wenliang},
	month = dec,
	year = {2017},
	note = {arXiv:1712.03214 [nucl-ex]},
	keywords = {Nuclear Experiment},
	annote = {Comment: 212 pages, Ph.D. thesis, University of Regina},
	file = {arXiv Fulltext PDF:C\:\\Users\\papatrott\\Zotero\\storage\\W7C7EWRG\\Li - 2017 - Exclusive Backward-Angle Omega Meson Electroproduc.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\papatrott\\Zotero\\storage\\LXR2AIU5\\1712.html:text/html},
}

