	file = {Accepted Version:C\:\\Users\\papatrott\\Zotero\\storage\\JW8SBGZL\\JLAB FPI-2 and E93-018 Collaboration et al. - 2018 - Separated kaon electroproduction cross section and.pdf:application/pdf;APS Snapshot:C\:\\Users\\papatrott\\Zotero\\storage\\34AEFATA\\PhysRevC.97.html:text/html;Full Text PDF:C\:\\Users\\papatrott\\Zotero\\storage\\72JUX7LX\\Carmignotto et al. - 2018 - Separated kaon electroproduction cross section and.pdf:application/pdf},
