	keywords = {Nuclear Experiment, High Energy Physics - Lattice, High Energy Physics - Phenomenology, Nuclear Theory},
	pages = {022003},
	annote = {Comment: 53 pages, 17 figures. Summary of three lectures delivered at "Hadron Physics XIII - XIII International Workshop on Hadron Physics", 22-27 March 2015, Hotel do Bosque, Angra dos Reis - RJ, Brazil},
	file = {IOP Full Text PDF:C\:\\Users\\papatrott\\Zotero\\storage\\4RRPVWQX\\Roberts - 2016 - Three Lectures on Hadron Physics.pdf:application/pdf;Roberts - 2016 - Three Lectures on Hadron Physics.pdf:C\:\\Users\\papatrott\\Zotero\\storage\\U7FUW2PQ\\Roberts - 2016 - Three Lectures on Hadron Physics.pdf:application/pdf},
